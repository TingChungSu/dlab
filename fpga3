module
top(
    input CLK,
    input RESET,
    input KDAT,
    input KCLK,
    input
 [3:0] SWH,
    output [7:0]LED,
    output hsync,
    output vsync,
    output
R,
    output G,
    output B
    );

    wire visible;
    wire
word,word1,word2,word3,word4,word_region,atk_region;
    wire [9:0]  row;
    wire
[9:0]  row_2;
    wire [10:0] col;
    wire [10:0] col_2;

    reg [2:0]
 pic_cnt;//Control your pic NO.

    reg row_inc;

    reg [2:0]  RGB_buff;
    reg
[9:0]  pixel_row;
    reg [16:0] counter;
    reg [10:0] pixel_col;
    reg [10:0]
center_x , center_y;
    reg [10:0] center_xx , center_yy;
    reg
SWH_old;
    assign LED = 8'b11111111;
    assign col = pixel_col -
center_x;
    assign col_2 = pixel_col - center_xx;
    assign row = pixel_row -
center_y;
    assign row_2 = pixel_row - center_yy;
    wire [7:0]DATA;
    wire
PS2_STATE;

    keyboard kk( .clk(CLK), .reset(RESET), .KCLK(KCLK), .KDAT(KDAT),
.DATA(DATA));
    WORD word_1(.word(word),
.word1(word1),.word2(word2),.word3(word3),.pic_cnt(pic_cnt), .row(row[6:3]),
.col(col[6:3]), .reset(RESET));
    ATK
word_2(.word(word4),.pic_cnt(pic_cnt),.row(row_2[6:3]), .col(col_2[6:3]),
.reset(RESET));
    always@( posedge CLK or posedge RESET ) begin
        if( RESET )
counter <= 0;
        else counter<=counter+1;
    end
    always@(posedge
CLK)begin
        if(RESET)   SWH_old <= SWH[0];
        else SWH_old <=
SWH[0];
    end




//======choose your pic num.=========

    always@(
posedge CLK or posedge RESET ) begin
        if( RESET ) pic_cnt <= 0;
        else
if(!SWH[0]) pic_cnt <=0;
        else if(SWH[0]) pic_cnt
<=1;
    end

//===========Region==================

    assign word_region = (
pixel_col >= center_x ) & ( pixel_col < center_x + 128 ) &
                         ( pixel_row
>= center_y ) & ( pixel_row < center_y + 128 );
    assign atk_region = (
pixel_col >= center_xx ) & ( pixel_col < center_xx + 128 ) &
                         (
pixel_row >= center_yy ) & ( pixel_row < center_yy + 128 );


 //-------------------//
    //   WORD POSITION
//
    //-------------------//


    always@( posedge CLK or posedge RESET)
begin
        if( RESET ) RGB_buff <= 3'b000;
        else if( word_region & word )
RGB_buff <= { 1'b0, 1'b0, 1'b0 };
        else if( word_region & word1 ) RGB_buff
<= { 1'b1, 1'b0, 1'b0 };
        else if( word_region & word2 ) RGB_buff <= { 1'b1,
1'b1, 1'b1 };
        else if( word_region & word3 ) RGB_buff <= { 1'b1, 1'b1, 1'b0
};
        else if( atk_region & word4 ) RGB_buff <= { 1'b0, 1'b1, 1'b1
};

        //Modify

        else RGB_buff <= 3'b101;
    end

    // RGB

    assign R = (
visible )? RGB_buff[2] : 0;
    assign G = ( visible )? RGB_buff[1] : 0;
    assign
B = ( visible )? RGB_buff[0] : 0;







 //===================Location===========================


    always
@(posedge CLK or posedge RESET)begin
        if( RESET ) center_y <= 200;//set your
location
        else if(center_y>23 && DATA==8'h1D &&
counter==0)center_y<=center_y-1;
        else if(center_y<495 && DATA==8'h1B &&
counter==0)center_y<=center_y+1;
    end

    always @(posedge CLK or posedge
RESET)begin
        if( RESET) center_x <= 300;//set your location
        else
if(center_x>104 &&  DATA==8'h1C && counter==0)center_x<=center_x-1;
        else
if(center_x<776 &&  DATA==8'h23 &&
counter==0)center_x<=center_x+1;
    end

    always @(posedge CLK or posedge
RESET)begin
        if( RESET ) center_yy <= 200;//set your location
        else
if(center_xx>776 )center_yy<=center_y;
        else
if(SWH_old!=SWH[0])center_yy<=center_y;
    end

    always @(posedge CLK or
posedge RESET)begin
        if( RESET) center_xx <= 428;//set your location
        else
if(center_xx>776)center_xx<=center_x+128;
        else
if(SWH_old!=SWH[0])center_xx<=center_x+128;
        else
if(counter==0)center_xx<=center_xx+1;
    end



    //---------------//
    //
  SCAN      //
    //---------------//

    always@( posedge CLK ) begin
        if(
RESET ) pixel_col <=0;
        else if( pixel_col == 1039 ) pixel_col <= 0;
        else
pixel_col <= pixel_col + 1;
    end

    always@( posedge CLK ) begin
        if( RESET
) row_inc <= 0;
        else if( pixel_col == 1039 ) row_inc <= 1;
        else row_inc
<= 0;
    end

    always@( posedge CLK )begin
        if( RESET ) pixel_row <=
0;
        else if ( pixel_row == 665 ) pixel_row <= 0;
        else if ( row_inc )
pixel_row <= pixel_row + 1;
        else pixel_row <= pixel_row;
    end

    assign
hsync = ~( (pixel_col >= 919) & (pixel_col < 1039) );
    assign vsync = ~(
(pixel_row >= 659) & (pixel_row < 665) ) ;
    assign visible = ( (pixel_col >
104) & (pixel_col < 904 ) & (pixel_row > 23) & (pixel_row < 623)
);

            /*
            line_a <= 16'b0000000000000000;
            line_b <=
16'b0000000000000000;
            line_c <= 16'b0000000000000000;
            line_d <=
16'b0001100000011000;
            line_e <= 16'b0001100000011000;
            line_f <=
16'b0001100000011000;
            line_g <= 16'b0001100000011000;
            line_h <=
16'b0001100000011000;
            line_i <= 16'b0000000000000000;
            line_j <=
16'b1100000000000011;
            line_k <= 16'b0111000000001110;
            line_l <=
16'b0001110000111000;
            line_m <= 16'b0000011111100000;
            line_n <=
16'b0000000110000000;
            line_o <= 16'b0000000000000000;
            line_p <=
16'b0000000000000000;*/

endmodule

