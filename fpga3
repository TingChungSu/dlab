//BLACK
always@( pic_cnt ) begin
    case( pic_cnt )
        0: begin
            line_a <=
16'b0000111111000000;
            line_b <= 16'b0000000000000000;
            line_c <=
16'b0001111001000000;
            line_d <= 16'b0001010001000000;
            line_e <=
16'b0001000000000000;
            line_f <= 16'b0001100000000000;
            line_g <=
16'b0000000000000000;
            line_h <= 16'b0000000000000000;
            line_i <=
16'b0000000000000000;
            line_j <= 16'b0000000000000000;
            line_k <=
16'b0000000000000000;
            line_l <= 16'b0000011111100000;
            line_m <=
16'b0000001010000000;
            line_n <= 16'b0000000000000000;
            line_o <=
16'b0000000000000000;
            line_p <= 16'b0000000000000000;
           end

 1:begin
            line_a <= 16'b0000111111000000;
            line_b <=
16'b0000000000000000;
            line_c <= 16'b0001111001000000;
            line_d <=
16'b0001010001000000;
            line_e <= 16'b0001000000000000;
            line_f <=
16'b0001100000000000;
            line_g <= 16'b0000000000000000;
            line_h <=
16'b0000000000000000;
            line_i <= 16'b0000000000000000;
            line_j <=
16'b0000000000000000;
            line_k <= 16'b0000000000000000;
            line_l <=
16'b0000011111100000;
            line_m <= 16'b0000001010000000;
            line_n <=
16'b0000000000000000;
            line_o <= 16'b0000000000000000;
            line_p <=
16'b0000000000000000;
            end
    endcase
end

