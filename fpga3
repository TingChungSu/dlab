        8'b1110_1110: word <= line_o[1];
        8'b1110_1111: word <=
line_o[0];

        8'b1111_0000: word <= line_p[15];
        8'b1111_0001: word <=
line_p[14];
        8'b1111_0010: word <= line_p[13];
        8'b1111_0011: word <=
line_p[12];
        8'b1111_0100: word <= line_p[11];
        8'b1111_0101: word <=
line_p[10];
        8'b1111_0110: word <= line_p[9];
        8'b1111_0111: word <=
line_p[8];
        8'b1111_1000: word <= line_p[7];
        8'b1111_1001: word <=
line_p[6];
        8'b1111_1010: word <= line_p[5];
        8'b1111_1011: word <=
line_p[4];
        8'b1111_1100: word <= line_p[3];
        8'b1111_1101: word <=
line_p[2];
        8'b1111_1110: word <= line_p[1];
        8'b1111_1111: word <=
line_p[0];
    endcase
end
endmodule

module WORD(
  //output
  word,
  word1,

 word2,
  word3,
  //input
  pic_cnt, row, col, reset);
input [3:0]
row;
input [3:0] col;
input [2:0] pic_cnt;
input reset ;
output
word,word1,word2,word3;
reg word,word1,word2,word3;
reg [15:0]
line_a,line_b,line_c,line_d,line_e,line_f,line_g,line_h,

line_i,line_j,line_k,line_l,line_m,line_n,line_o,line_p,


